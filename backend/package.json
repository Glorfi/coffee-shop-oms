{
  "name": "aiexgen-be",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "husky": {
    "hook": {
      "pre-commit": "npm run lint"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf build && tsc",
    "add-build": "git add build",
    "local": "node --no-warnings --loader ts-node/esm src/index.ts",
    "local-network": "node --no-warnings --loader ts-node/esm src/index.ts --host 192.168.43.59 --port 4000",
    "dev": "nodemon --watch src/ -e ts --exec \"npm run local\"",
    "dev2": "nodemon --watch src/ -e ts --exec \"npm run local-network\"",
    "start": "npm run build && node build/index",
    "ts.check": "tsc --project tsconfig.json",
    "lint": "npx eslint ./src",
    "format": "npx eslint ./src --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "dev": "ts-node-dev --respawn --transpile-only --loader ts-node/esm src/index.ts",
    "eslint": "^8.56.0",
    "husky": "^9.0.10",
    "nodemon": "^3.0.3",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "celebrate": "^15.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "joi": "^17.12.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.1.1",
    "node-fetch": "^3.3.2",
    "pre-commit": "^1.2.2",
    "socket.io": "^4.7.5"
  },
  "pre-commit": [
    "ts.check",
    "build",
    "add-build"
  ]
}
